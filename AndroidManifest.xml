<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sa.growonline.footprint">

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name="sa.growonline.footprint.ZuniApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="sa.growonline.footprint.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityLogin"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivitySingleProduct"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityWishListDetails"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityCheckOut"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivitySignUp"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityGiftCard"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityCartDetails"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:name="sa.growonline.footprint.ActivityProductList"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="com.urbanairship.push.iam.EXCLUDE_FROM_AUTO_SHOW"
                android:value="true" />
        </activity>
        <activity
            android:name="sa.growonline.footprint.ActivityProductDetail"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <meta-data
                android:name="com.urbanairship.push.iam.EXCLUDE_FROM_AUTO_SHOW"
                android:value="true" />
        </activity>
        <activity
            android:name="sa.growonline.footprint.ActivityImageDetail"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivitySubCategory"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityMyAccount"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityRewardPoints"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityAddresses"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivitySplash"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <meta-data
                android:name="com.urbanairship.push.iam.EXCLUDE_FROM_AUTO_SHOW"
                android:value="true" />
        </activity>
        <activity
            android:name="sa.growonline.footprint.ActivityHomeNew"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />

        <activity
            android:name="sa.growonline.footprint.ActivityContact"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityCustomerInfo"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityOrderDetail"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityOrderListing"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityChangePassword"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />

        <activity
            android:name="sa.growonline.footprint.ActivityShopTheLook"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />


        <activity
            android:name="sa.growonline.footprint.ActivityAddressEntry"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityOrder"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />

        <activity
            android:name="sa.growonline.footprint.ActivityGroupProductDetail"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityWebView"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />
        <activity
            android:name="sa.growonline.footprint.ActivityParallaxHome"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />

        <activity
            android:name="sa.growonline.footprint.DialoguePlus"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />

        <activity
            android:name="sa.growonline.footprint.ActivityBrand"
            android:label="@string/title_activity_activity_splash"
            android:screenOrientation="portrait" />


        <meta-data
            android:name="com.urbanairship.autopilot"
            android:value="com.urbanairship.Autopilot" />

        <!-- OPTIONAL, if you want to receive push, push opened and registration completed intents -->
        <!-- Replace the receiver below with your package and class name -->
        <receiver
            android:name=".urbanair.UrbanAirReceiver"
            android:exported="false">

            <intent-filter>
                <action android:name="com.urbanairship.push.CHANNEL_UPDATED" />
                <action android:name="com.urbanairship.push.OPENED" />
                <action android:name="com.urbanairship.push.RECEIVED" />
                <action android:name="com.urbanairship.push.DISMISSED" />

                <category android:name="sa.growonline.footprint" />
            </intent-filter>
        </receiver>

    </application>


</manifest>